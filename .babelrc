{
  "plugins": [
    "macros",
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "styles": "./styles",
          "context": "./context",
          "static": "./static"
        }
      }
    ]
  ],
  "presets": [
    "next/babel"
  ]
}